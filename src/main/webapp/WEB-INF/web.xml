<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >
 
<web-app>
  <icon>
  	<small-icon>src/main/resources/favicon.ico</small-icon>
	<large-icon>src/main/resources/favicon.ico</large-icon>
  </icon>
  <display-name>Archetype Created Web Application</display-name>
  <filter>
  	<filter-name>AuthenticationFilter</filter-name>
  	<display-name>AuthenticationFilter</display-name>
  	<description></description>
  	<filter-class>controller.AuthenticationFilter</filter-class>
  </filter>
  <filter-mapping>
  	<filter-name>AuthenticationFilter</filter-name>
  	<url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
  	<servlet-name>Session</servlet-name>
  	<display-name>Session</display-name>
  	<description></description>
  	<servlet-class>restController.SessionController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Solve</servlet-name>
  	<display-name>Solve</display-name>
  	<description></description>
  	<servlet-class>restController.SolveController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Cube</servlet-name>
  	<display-name>Cube</display-name>
  	<description></description>
  	<servlet-class>restController.CubeController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>User</servlet-name>
  	<display-name>User</display-name>
  	<description></description>
  	<servlet-class>controller.UserController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Session</servlet-name>
  	<url-pattern>/session/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Solve</servlet-name>
  	<url-pattern>/solve/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Cube</servlet-name>
  	<url-pattern>/cube/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>User</servlet-name>
  	<url-pattern>/user/*</url-pattern>
  </servlet-mapping>
</web-app>
